{
  "Boolean variable": {
    "body": "variable ${1} {\n  type        = bool\n  default     = ${2|true,false,null|}\n  description = \"${3}\"\n}\n",
    "description": "Input variable with `bool` type, `default` value and `description`.",
    "prefix": "variable-bool",
    "scope": "source.tf"
  },
  "Datasource": {
    "body": "data ${1} ${2} {\n  ${0}\n}\n",
    "description": "Simple `data` block.",
    "prefix": "data",
    "scope": "source.tf"
  },
  "List strings variable": {
    "body": "variable ${1} {\n  type        = list(string)\n  default     = ${2|[],null|}\n  description = \"${3}\"\n}\n",
    "description": "Input variable with list of string, `default` value and `description`.",
    "prefix": "variable-list-strings",
    "scope": "source.tf"
  },
  "Local module": {
    "body": "module ${1} {\n  source = \"./modules/${1}\"\n  ${2}   = ${0}\n}\n",
    "description": "Child module configuration block placed in local `modules` directory.",
    "prefix": "module-local",
    "scope": "source.tf"
  },
  "Local values": {
    "body": "locals {\n  ${1} = ${2}\n}\n",
    "description": "A set of related local values.",
    "prefix": "locals",
    "scope": "source.tf"
  },
  "Map variable": {
    "body": "variable ${1} {\n  type        = map(any)\n  default     = ${2|{},null|}\n  description = \"${3}\"\n}\n",
    "description": "Input variable with map, `default` value and `description`.",
    "prefix": "variable-map",
    "scope": "source.tf"
  },
  "Numeric variable": {
    "body": "variable ${1} {\n  type        = number\n  default     = ${2|-1,0,1,null|}\n  description = \"${3}\"\n}\n",
    "description": "Input variable with `number` type, `default` value and `description`.",
    "prefix": "variable-number",
    "scope": "source.tf"
  },
  "Output": {
    "body": "output ${1} {\n  value       = ${2}\n  description = \"${3}\"\n}\n",
    "description": "Output value with `description`.",
    "prefix": "output",
    "scope": "source.tf"
  },
  "Output value depended": {
    "body": "output ${1} {\n  value       = ${2}\n  description = \"${3}\"\n  depends_on  = [\n    ${0}\n  ]\n}\n",
    "description": "Output value with `description` with dependencies.",
    "prefix": "output-depends",
    "scope": "source.tf"
  },
  "Registry module": {
    "body": "module ${1} {\n  source  = \"${2}/${1}/${3}\"\n  version = \"${4|1.0.0|}\"\n  ${5}    = ${0}\n}\n",
    "description": "Includes child module from Terraform registry.",
    "prefix": "module-registry",
    "scope": "source.tf"
  },
  "Resource": {
    "body": "resource ${1} ${2} {\n  ${0}\n}\n",
    "description": "Simple `resource` block.",
    "prefix": "resource",
    "scope": "source.tf"
  },
  "Sensitive output": {
    "body": "output ${1} {\n  value       = ${2}\n  description = \"${3}\"\n  sensitive   = true\n}\n",
    "description": "Output value with `description` (sensitive content).",
    "prefix": "output-sensitive",
    "scope": "source.tf"
  },
  "String variable": {
    "body": "variable ${1} {\n  type        = string\n  default     = ${2|\"\",null|}\n  description = \"${3}\"\n}\n",
    "description": "Input variable with `string` type, `default` value and `description`.",
    "prefix": "variable-string",
    "scope": "source.tf"
  },
  "Terraform": {
    "body": "terraform {\n  required_version = \"\u003e= 0.12\"\n}\n",
    "description": "Simple terraform block with required version.",
    "prefix": "terraform",
    "scope": "source.tf"
  }
}
