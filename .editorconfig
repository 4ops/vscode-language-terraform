# http://editorconfig.org
root = true

# editorconfig

[*]
indent_style = space
indent_size = 2
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_spaces = false
max_line_length = 80

[*.sh]
indent_style = space
indent_size = 4

[*.go]
indent_style = tab
indent_size = 8
